{
  "manifest_version": 2,

  "name": "Joyfeed",
  "description": "Joyify your social feed",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": true
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab",
    "background",
    "https://ajax.googleapis.com/",
    "https://api.imgur.com/",
    "https://api.projectoxford.ai/"
  ]
}
